{
  "lerna": "2.9.0",
  "version": "0.0.25",
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "allowBranch": "develop",
      "message": "chore(release): publish %s"
    }
  },
  "changelog": {
    "repo": "risio/framework",
    "labels": {
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "docs": "Documentation :book:"
    },
    "cacheDir": ".changelog"
  }
}
